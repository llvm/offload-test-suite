    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 18
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [3536, 4, 0, 3552, 4, 0, 7168, 4, 0, 3536, 4, 0, 3552, 4, 0, 7168, 4, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
