  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 66
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 5696, 1426063360, 0, 5696, 1426063360, 0, 5696, 1426063360, 0, 5696, 1426063360, 0, 7184, 1, 0, 7200, 1, 0, 7216, 1, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 5696, 1426063360, 0, 5696, 1426063360, 0, 5696, 1426063360, 0, 5696, 1426063360, 0, 7184, 1, 0, 7200, 1, 0, 7216, 1, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
