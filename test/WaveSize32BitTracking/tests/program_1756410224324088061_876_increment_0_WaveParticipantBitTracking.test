    Data: [6784, 2048, 0, 8128, 1409286145, 0, 8128, 1409286145, 0, 8128, 1409286145, 0, 8128, 1409286145, 0, 6784, 2048, 0, 8128, 1409286145, 0, 8128, 1409286145, 0, 8128, 1409286145, 0, 8128, 1409286145, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
