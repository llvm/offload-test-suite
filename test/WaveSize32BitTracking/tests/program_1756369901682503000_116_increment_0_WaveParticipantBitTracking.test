    Data: [576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 3536, 268435456, 0, 3552, 268435456, 0, 5136, 2080, 0, 5136, 2080, 0, 5152, 2080, 0, 5152, 2080, 0, 6464, 17, 0, 6464, 17, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 3536, 268435456, 0, 3552, 268435456, 0, 5136, 2080, 0, 5136, 2080, 0, 5152, 2080, 0, 5152, 2080, 0, 6464, 17, 0, 6464, 17, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
