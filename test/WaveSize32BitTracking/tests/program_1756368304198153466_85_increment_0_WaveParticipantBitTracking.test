        if (((((WaveGetLaneIndex() == 6) || (WaveGetLaneIndex() == 16)) || (WaveGetLaneIndex() == 28)) || (WaveGetLaneIndex() == 7))) {
          result = (result + WaveActiveMax(result));
          uint temp = 0;
