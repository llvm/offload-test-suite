    Stride: 4
    Fill: 0
    Size: 42
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1872, 9, 0, 1872, 9, 0, 1888, 9, 0, 1888, 9, 0, 1904, 9, 0, 1904, 9, 0, 3152, 9, 0, 3152, 9, 0, 3168, 9, 0, 3168, 9, 0, 3184, 9, 0, 3184, 9, 0, 8960, 4, 0, 9536, 4, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
