    Stride: 4
    Fill: 0
    Size: 90
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1856, 7, 0, 1856, 7, 0, 1856, 7, 0, 3728, 1, 0, 3732, 1, 0, 3736, 1, 0, 3744, 1, 0, 3748, 1, 0, 3752, 1, 0, 3760, 1, 0, 3764, 1, 0, 3768, 1, 0, 4304, 1, 0, 4320, 1, 0, 4336, 1, 0, 4880, 4, 0, 4896, 4, 0, 4912, 4, 0, 5520, 8, 0, 5536, 8, 0, 5552, 8, 0, 6416, 4, 0, 6432, 4, 0, 6448, 4, 0, 7120, 10, 0, 7120, 10, 0, 7136, 10, 0, 7136, 10, 0, 7152, 10, 0, 7152, 10, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
