  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]  # Single dispatch for 4 threads
Buffers:
  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 33
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [3392, 9, 0, 3392, 9, 0, 4288, 4, 0, 4928, 1, 0, 5824, 4, 0, 6992, 8, 0, 7008, 8, 0, 7024, 8, 0, 8784, 8, 0, 8800, 8, 0, 8816, 8, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
  - Resources:
    - Name: _participant_bit
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
