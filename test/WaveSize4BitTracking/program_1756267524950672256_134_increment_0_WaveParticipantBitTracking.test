    Stride: 4
    Fill: 0
    Size: 63
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [576, 1, 0, 4880, 2, 0, 4896, 2, 0, 5840, 2, 0, 5856, 2, 0, 6784, 4, 0, 7232, 8, 0, 9236, 1, 0, 9240, 1, 0, 9252, 1, 0, 9256, 1, 0, 9684, 1, 0, 9688, 1, 0, 9700, 1, 0, 9704, 1, 0, 11216, 14, 0, 11216, 14, 0, 11216, 14, 0, 11232, 14, 0, 11232, 14, 0, 11232, 14, 0]
  - Name: _wave_op_index
