    Stride: 4
    Fill: 0
    Size: 39
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1088, 9, 0, 1088, 9, 0, 1664, 4, 0, 3776, 4, 0, 6528, 2, 0, 7424, 10, 0, 7424, 10, 0, 9680, 10, 0, 9680, 10, 0, 9696, 10, 0, 9696, 10, 0, 9712, 10, 0, 9712, 10, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
