  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]  # Single dispatch for 4 threads
Buffers:
  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 81
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [576, 1, 0, 2064, 4, 0, 2080, 4, 0, 2704, 4, 0, 2720, 4, 0, 4736, 8, 0, 5632, 5, 0, 5632, 5, 0, 6592, 3, 0, 6592, 3, 0, 6608, 3, 0, 6608, 3, 0, 6624, 3, 0, 6624, 3, 0, 7232, 2, 0, 7248, 2, 0, 7264, 2, 0, 7680, 12, 0, 7680, 12, 0, 7696, 12, 0, 7696, 12, 0, 7712, 12, 0, 7712, 12, 0, 8320, 5, 0, 8320, 5, 0, 8896, 5, 0, 8896, 5, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
  - Resources:
    - Name: _participant_bit
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
        Binding: 0
    - Name: _wave_op_index
      Kind: RWStructuredBuffer
