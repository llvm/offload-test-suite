    Data: [896, 4, 0, 912, 4, 0, 2500, 1, 0, 2504, 1, 0, 2508, 1, 0, 2516, 1, 0, 2520, 1, 0, 2524, 1, 0, 3204, 5, 0, 3204, 5, 0, 3208, 5, 0, 3208, 5, 0, 3212, 5, 0, 3212, 5, 0, 3220, 5, 0, 3220, 5, 0, 3224, 5, 0, 3224, 5, 0, 3228, 5, 0, 3228, 5, 0, 4036, 13, 0, 4036, 13, 0, 4036, 13, 0, 4040, 13, 0, 4040, 13, 0, 4040, 13, 0, 4044, 13, 0, 4044, 13, 0, 4044, 13, 0, 4052, 13, 0, 4052, 13, 0, 4052, 13, 0, 4056, 13, 0, 4056, 13, 0, 4056, 13, 0, 4060, 13, 0, 4060, 13, 0, 4060, 13, 0, 4608, 2, 0, 4624, 2, 0]
  - Name: _wave_op_index
