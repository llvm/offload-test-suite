    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 30
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [576, 1, 0, 2240, 4, 0, 2256, 4, 0, 2272, 4, 0, 13696, 6, 0, 13696, 6, 0, 13440, 9, 0, 13440, 9, 0, 14336, 5, 0, 14336, 5, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
