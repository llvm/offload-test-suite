    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 15
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1664, 4, 0, 1280, 8, 0, 1024, 1, 0, 2688, 1, 0, 9216, 4, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
