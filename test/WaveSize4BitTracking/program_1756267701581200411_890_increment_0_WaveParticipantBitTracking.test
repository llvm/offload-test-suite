    Data: [576, 1, 0, 3024, 4, 0, 3028, 4, 0, 3040, 4, 0, 3044, 4, 0, 3056, 4, 0, 3060, 4, 0, 3600, 4, 0, 3616, 4, 0, 3632, 4, 0, 6528, 8, 0, 7168, 5, 0, 7168, 5, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
