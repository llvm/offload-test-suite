    Stride: 4
    Data: [1296, 1, 0, 1312, 1, 0, 1328, 1, 0, 2388, 5, 0, 2388, 5, 0, 2392, 5, 0, 2392, 5, 0, 2404, 5, 0, 2404, 5, 0, 2408, 5, 0, 2408, 5, 0, 2420, 5, 0, 2420, 5, 0, 2424, 5, 0, 2424, 5, 0, 3668, 5, 0, 3668, 5, 0, 3672, 5, 0, 3672, 5, 0, 3684, 5, 0, 3684, 5, 0, 3688, 5, 0, 3688, 5, 0, 3700, 5, 0, 3700, 5, 0, 3704, 5, 0, 3704, 5, 0, 4560, 5, 0, 4560, 5, 0, 4576, 5, 0, 4576, 5, 0, 4592, 5, 0, 4592, 5, 0, 5120, 5, 0, 5120, 5, 0]
  - Name: _wave_op_index
