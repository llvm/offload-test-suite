Shaders:
  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]  # Single dispatch for 4 threads
Buffers:
  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 96
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1088, 13, 0, 1088, 13, 0, 1088, 13, 0, 2256, 13, 0, 2256, 13, 0, 2256, 13, 0, 2272, 13, 0, 2272, 13, 0, 2272, 13, 0, 3920, 12, 0, 3920, 12, 0, 3936, 12, 0, 3936, 12, 0, 4880, 13, 0, 4880, 13, 0, 4880, 13, 0, 4896, 13, 0, 4896, 13, 0, 4896, 13, 0, 6480, 13, 0, 6480, 13, 0, 6480, 13, 0, 6496, 13, 0, 6496, 13, 0, 6496, 13, 0, 7168, 13, 0, 7168, 13, 0, 7168, 13, 0, 10816, 6, 0, 10816, 6, 0, 10560, 8, 0, 10304, 1, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
