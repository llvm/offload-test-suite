        }
      case 1: {
          for (uint i1 = 0; (i1 < 2); i1 = (i1 + 1)) {
            if ((WaveGetLaneIndex() >= 2)) {
              result = (result + WaveActiveMax((WaveGetLaneIndex() + 4)));
