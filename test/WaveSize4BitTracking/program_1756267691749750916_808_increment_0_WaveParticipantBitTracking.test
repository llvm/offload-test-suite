  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]  # Single dispatch for 4 threads
Buffers:
  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 57
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [2112, 4, 0, 3008, 9, 0, 3008, 9, 0, 3904, 4, 0, 5328, 14, 0, 5328, 14, 0, 5328, 14, 0, 5344, 14, 0, 5344, 14, 0, 5344, 14, 0, 5360, 14, 0, 5360, 14, 0, 5360, 14, 0, 6288, 5, 0, 6288, 5, 0, 6304, 5, 0, 6304, 5, 0, 6320, 5, 0, 6320, 5, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
  - Resources:
    - Name: _participant_bit
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
