    Data: [832, 10, 0, 832, 10, 0, 1472, 1, 0, 2368, 4, 0, 4160, 4, 0, 5376, 4, 0, 5392, 4, 0, 8000, 1, 0, 9792, 1, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
