  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 96
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [576, 5, 0, 576, 5, 0, 1728, 8, 0, 6464, 8, 0, 7936, 6, 0, 7936, 6, 0, 7952, 6, 0, 7952, 6, 0, 9280, 1, 0, 9296, 1, 0, 9312, 1, 0, 10688, 1, 0, 10692, 1, 0, 10704, 1, 0, 10708, 1, 0, 10720, 1, 0, 10724, 1, 0, 11328, 1, 0, 11344, 1, 0, 11360, 1, 0, 12544, 10, 0, 12544, 10, 0, 12560, 10, 0, 12560, 10, 0, 12576, 10, 0, 12576, 10, 0, 13696, 9, 0, 13696, 9, 0, 13712, 9, 0, 13712, 9, 0, 13728, 9, 0, 13728, 9, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
  - Resources:
    - Name: _participant_bit
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
