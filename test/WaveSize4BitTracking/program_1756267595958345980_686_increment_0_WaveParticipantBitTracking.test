    Fill: 0
    Size: 57
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1344, 1, 0, 1360, 1, 0, 4224, 1, 0, 4240, 1, 0, 5184, 1, 0, 5188, 1, 0, 5192, 1, 0, 5200, 1, 0, 5204, 1, 0, 5208, 1, 0, 5632, 1, 0, 5636, 1, 0, 5640, 1, 0, 5648, 1, 0, 5652, 1, 0, 5656, 1, 0, 7104, 1, 0, 7424, 4, 0, 7872, 8, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
