    Fill: 0
    Size: 57
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [832, 10, 0, 832, 10, 0, 1936, 4, 0, 1952, 4, 0, 1968, 4, 0, 2896, 1, 0, 2900, 1, 0, 2904, 1, 0, 2912, 1, 0, 2916, 1, 0, 2920, 1, 0, 2928, 1, 0, 2932, 1, 0, 2936, 1, 0, 3344, 1, 0, 3360, 1, 0, 3376, 1, 0, 4352, 10, 0, 4352, 10, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
