  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1296, 8, 0, 1312, 8, 0, 2000, 8, 0, 2016, 8, 0, 4288, 1, 0, 5696, 1, 0, 6336, 9, 0, 6336, 9, 0, 7232, 4, 0, 9024, 9, 0, 9024, 9, 0, 8768, 4, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
