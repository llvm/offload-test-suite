    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 51
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1040, 1, 0, 1056, 1, 0, 1072, 1, 0, 1680, 1, 0, 1696, 1, 0, 1712, 1, 0, 2128, 1, 0, 2144, 1, 0, 2160, 1, 0, 3008, 4, 0, 3904, 5, 0, 3904, 5, 0, 4480, 5, 0, 4480, 5, 0, 5376, 5, 0, 5376, 5, 0, 7760, 8, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
  - Resources:
    - Name: _participant_bit
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 2
        Space: 0
      VulkanBinding:
        Binding: 2
    - Name: _wave_op_index
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 3
        Space: 0
      VulkanBinding:
        Binding: 3
...
#--- end

