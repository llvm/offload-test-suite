#--- source.hlsl
struct Level1 {
  uint a;
};

struct Level2 {
  Level1 l1;
  uint b;
};

struct Level3 {
  Level2 l2;
  uint c;
};

[[vk::push_constant]] Level3 buffer;

RWStructuredBuffer<uint> output : register(u0);

[numthreads(1,1,1)]
void main() {
  output[0] = buffer.l2.l1.a;
  output[1] = buffer.l2.b;
  output[2] = buffer.c;
}
//--- pipeline.yaml
---
Shaders:
  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]
Buffers:
  - Name: output
    Format: Int32
    Data: [ 0, 0, 0 ]
PushConstants:
  - Stage: Compute
    Values:
    - Format: Int32
      Offset: 0
      Data: [ 10 ]
    - Format: Int32
      Offset: 4
      Data: [ 20 ]
    - Format: Int32
      Offset: 8
      Data: [ 30 ]
DescriptorSets:
  - Resources:
    - Name: output
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
        Binding: 0
...
#--- end
# UNSUPPORTED: Metal || DirectX

# RUN: split-file %s %t
# RUN: %dxc_target -fvk-use-dx-layout -T cs_6_0 -Fo %t.o %t/source.hlsl
# RUN: %offloader %t/pipeline.yaml %t.o | FileCheck %s

# CHECK: Name: output
# CHECK: Data: [
# CHECK: 10,
# CHECK: 20,
# CHECK: 30
# CHECK: ]
