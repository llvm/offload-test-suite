#--- source.hlsl
struct S {
  uint a;
  uint b;

  uint sum() {
    return a + b;
  }
};

[[vk::push_constant]] S buffer;

RWStructuredBuffer<uint> output : register(u0);

[numthreads(1,1,1)]
void main() {
  output[0] = buffer.a;
  output[1] = buffer.b;
  output[2] = buffer.sum();
}
//--- pipeline.yaml
---
Shaders:
  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]
Buffers:
  - Name: output
    Format: Int32
    Data: [ 0, 0, 0 ]
PushConstants:
  - Stage: Compute
    Values:
    - Format: Int32
      Offset: 0
      Data: [ 10, 20 ]
DescriptorSets:
  - Resources:
    - Name: output
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
        Binding: 0
...
#--- end
# UNSUPPORTED: Metal || DirectX

# RUN: split-file %s %t
# RUN: %dxc_target -fvk-use-dx-layout -T cs_6_0 -Fo %t.o %t/source.hlsl
# RUN: %offloader %t/pipeline.yaml %t.o | FileCheck %s

# CHECK: Name: output
# CHECK: Data: [
# CHECK: 10,
# CHECK: 20,
# CHECK: 30
# CHECK: ]
