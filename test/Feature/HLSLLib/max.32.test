  - Name: Y0
    Format: Float32
    Stride: 16
    Data: [ -1.0, 1.0, 1.5, 129.5, 2.71828, 42, -654.321, 0.0002, 10, 10.5, 0, -3e+38 ]
  - Name: X1
    Format: Int32
    Stride: 16
    Data: [ -2147483648, -10, 0, 0, 10, 2147483647, 1000, 2500, 1, -1, 512, -2048 ]
  - Name: Y1
