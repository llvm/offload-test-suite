#--- source.hlsl

StructuredBuffer<float4> In : register(t0);

RWStructuredBuffer<float4> Out : register(u1);

[numthreads(1,1,1)]
void main() {
  Out[0] = asin(In[0]);
  float4 Tmp = {asin(In[1].xyz), asin(In[1].w)};
  Out[1] = Tmp;
  float4 Tmp2 = {asin(In[2].xy), asin(In[2].zw)};
  Out[2] = Tmp2;
}


//--- pipeline.yaml

---
Shaders:
  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]
Buffers:
  - Name: In
    Format: Float32
    Stride: 16
    Data: [ nan, -inf, -0x1.e7d42cp-127, -0, 0, 0x1.e7d42cp-127, inf, 1, -1, 1.5, -1.5, nan,]
    #  NaN, -Inf, -denorm, -0, 0, denorm, Inf, 1, -1, 1.5, -1.5,
  - Name: Out
    Format: Float32
    Stride: 16
    FillSize: 48
  - Name: ExpectedOut # The result we expect
    Format: Float32
    Stride: 16
    Data: [ nan, nan, 0.0, 0.0, 0.0, 0.0, nan, 1.570796, -1.570796, nan, nan, nan,]
    #  NaN, NaN, 0.0, 0.0, 0.0, 0.0, NaN, 1.570796, -1.570796, NaN, NaN,
Results:
  - Result: Test1
    Rule: BufferFloatEpsilon
    Epsilon: 0.0008
    Actual: Out
    Expected: ExpectedOut
DescriptorSets:
  - Resources:
    - Name: In
      Kind: StructuredBuffer
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
        Binding: 0
    - Name: Out
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 1
        Space: 0
      VulkanBinding:
        Binding: 1
...
#--- end

# RUN: split-file %s %t
# RUN: %dxc_target -T cs_6_5 -Gis -Fo %t.o %t/source.hlsl
# RUN: %offloader %t/pipeline.yaml %t.o
