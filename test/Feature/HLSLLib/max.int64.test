    Stride: 32
    Data: [ 0, 256, 4, 10001, 0, 18446744073709551615, 1500, 2000, 0, 200, 511, 2047 ]
  - Name: Out0
    Format: Int64
    Stride: 32
    FillSize: 128
  - Name: ExpectedOut0
    Format: Int64
    Stride: 32
    Data: [ 0, 10, 0, 272, 15, 9223372036854775807, 1500, 2500, 1, 1, 512, -2047, 0, 10, 15, 9223372036854775807 ]
  - Name: Out1
    Format: UInt64
    Stride: 32
