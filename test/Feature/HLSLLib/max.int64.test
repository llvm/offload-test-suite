  - Name: Y0
    Format: Int64
    Stride: 32
    Data: [ 0, 10, -3114, 272, 15, 0, 1500, 2000, -1, 1, 511, -2047 ]
  - Name: X1
    Format: UInt64
    Stride: 32
    Data: [ 0, 0, 10, 10000, 9223372036854775807, 18446744073709551615, 1000, 2500, 1, 256, 512, 2048 ]
  - Name: Y1
