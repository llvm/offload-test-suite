#--- source.hlsl

cbuffer CBArrays : register(b0) {
  double2 c1[2];
  uint64_t3 c2[2];
  int64_t c3[2];
  uint64_t c4[2][2];
}

struct Arrays {
  double2 c1[2];
  uint64_t3 c2[2];
  int64_t c3[2];
  uint64_t c4[2][2];
};

RWStructuredBuffer<Arrays> Out : register(u1);

[numthreads(1,1,1)]
void main() {
  Out[0].c1 = c1;
  Out[0].c2 = c2;
  Out[0].c3 = c3;
  Out[0].c4 = c4;
}

