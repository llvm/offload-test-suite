#--- source.hlsl

cbuffer CBArrays : register(b0) {
  float c1[2];
  int4 c2[2][2];
  bool c3[2];
  int3 c4[2][2];
}

struct Arrays {
  float c1[2];
  int4 c2[2][2];
  bool c3[2];
  int3 c4[2][2];
};

RWStructuredBuffer<Arrays> Out : register(u1);

[numthreads(1,1,1)]
void main() {
  Out[0].c1 = c1;
  Out[0].c2 = c2;
  Out[0].c3 = c3;
  Out[0].c4 = c4;
}

