#--- duplicate_spec_id.hlsl
[[vk::constant_id(0)]]
const int spec_int_A = 0;
[[vk::constant_id(0)]]
const int spec_int_B = 0;

RWStructuredBuffer<int> Out : register(u0, space0);

[numthreads(1,1,1)]
void main(uint GI : SV_GroupIndex) {
  Out[0] = spec_int_A;
  Out[1] = spec_int_B;
}
