  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 9
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [6160, 2, 0, 6176, 2, 0, 6192, 2, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
  - Resources:
    - Name: _participant_bit
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
        Binding: 0
    - Name: _wave_op_index
      Kind: RWStructuredBuffer
