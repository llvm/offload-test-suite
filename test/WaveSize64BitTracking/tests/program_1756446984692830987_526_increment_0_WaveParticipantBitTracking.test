    Stride: 4
    Fill: 0
    Size: 3
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [3392, 4194304, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
