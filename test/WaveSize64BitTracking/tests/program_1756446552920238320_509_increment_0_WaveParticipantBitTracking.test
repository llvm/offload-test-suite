  case 2: {
      if (((WaveGetLaneIndex() < 17) || (WaveGetLaneIndex() >= 55))) {
        if (((WaveGetLaneIndex() < 6) || (WaveGetLaneIndex() >= 56))) {
          result = (result + WaveActiveMax((WaveGetLaneIndex() + 3)));
