[numthreads(64, 1, 1)]
void main(uint3 tid : SV_DispatchThreadID) {
  uint result = 0;
  switch ((WaveGetLaneIndex() % 3)) {
  case 0: {
      if (((((WaveGetLaneIndex() == 13) || (WaveGetLaneIndex() == 30)) || (WaveGetLaneIndex() == 44)) || (WaveGetLaneIndex() == 40))) {
