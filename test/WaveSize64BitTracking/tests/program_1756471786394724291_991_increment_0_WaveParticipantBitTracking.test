          }
          for (uint i2 = 0; (i2 < 3); i2 = (i2 + 1)) {
            if ((((WaveGetLaneIndex() == 19) || (WaveGetLaneIndex() == 33)) || (WaveGetLaneIndex() == 35))) {
              result = (result + WaveActiveSum(result));
              uint temp = 0;
