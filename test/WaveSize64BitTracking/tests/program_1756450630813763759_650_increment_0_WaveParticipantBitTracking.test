    Data: [1664, 17, 0, 1664, 17, 0, 2304, 34, 0, 2304, 34, 0, 3840, 34, 0, 3840, 34, 0, 11456, 559240, 0, 11456, 559240, 0, 11456, 559240, 0, 11456, 559240, 0, 11456, 559240, 0]
