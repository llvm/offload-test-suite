#--- source.hlsl
RWStructuredBuffer<int> Out : register(u0);

[numthreads(1,1,1)]
void main(uint GI : SV_GroupIndex) {
  Out.IncrementCounter();
  Out[GI] = 0;
}

#--- pipeline.yaml
---
Shaders:
  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]
Buffers:
  - Name: Out
    Format: Hex32
    Stride: 4
    FillSize: 4
DescriptorSets:
  - Resources:
    - Name: Out
      Kind: RWStructuredBuffer
      HasCounter: true
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
        Binding: 0
...
#--- end

# REQUIRES: Vulkan
# RUN: split-file %s %t
# RUN: %dxc_target -T cs_6_0 -Fo %t.o %t/source.hlsl
# RUN: not %offloader %t/pipeline.yaml %t.o 2>&1 | FileCheck %s
# CHECK: No CounterBinding provided for resource 'Out' with a counter
