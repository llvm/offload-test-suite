    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 54
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [576, 17, 0, 576, 17, 0, 1472, 17476, 0, 1472, 17476, 0, 1472, 17476, 0, 1472, 17476, 0, 6528, 34816, 0, 6528, 34816, 0, 6976, 32768, 0, 576, 17, 0, 576, 17, 0, 1472, 17476, 0, 1472, 17476, 0, 1472, 17476, 0, 1472, 17476, 0, 6528, 34816, 0, 6528, 34816, 0, 6976, 32768, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
