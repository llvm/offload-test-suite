    Stride: 4
    Fill: 0
    Size: 12
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1856, 16448, 0, 1856, 16448, 0, 1856, 16448, 0, 1856, 16448, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
