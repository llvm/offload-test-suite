    _participant_bit[(temp + 2)] = ballot.y;
  }
  if ((((WaveGetLaneIndex() == 0) || (WaveGetLaneIndex() == 9)) || (WaveGetLaneIndex() == 13))) {
    if (((((WaveGetLaneIndex() == 2) || (WaveGetLaneIndex() == 6)) || (WaveGetLaneIndex() == 13)) || (WaveGetLaneIndex() == 8))) {
      result = (result + WaveActiveMin((WaveGetLaneIndex() + 1)));
