    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 48
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [768, 73, 0, 768, 73, 0, 768, 73, 0, 5760, 1040, 0, 5760, 1040, 0, 6912, 16388, 0, 6912, 16388, 0, 8960, 4, 0, 768, 73, 0, 768, 73, 0, 768, 73, 0, 5760, 1040, 0, 5760, 1040, 0, 6912, 16388, 0, 6912, 16388, 0, 8960, 4, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
  - Resources:
    - Name: _participant_bit
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
        Binding: 0
    - Name: _wave_op_index
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 1
        Space: 0
