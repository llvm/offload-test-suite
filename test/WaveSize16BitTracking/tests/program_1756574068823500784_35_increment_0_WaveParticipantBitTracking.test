    Data: [8384, 33288, 0, 8384, 33288, 0, 8384, 33288, 0, 8960, 1040, 0, 8960, 1040, 0, 9600, 18724, 0, 9600, 18724, 0, 9600, 18724, 0, 9600, 18724, 0, 9600, 18724, 0, 8384, 33288, 0, 8384, 33288, 0, 8384, 33288, 0, 8960, 1040, 0, 8960, 1040, 0, 9600, 18724, 0, 9600, 18724, 0, 9600, 18724, 0, 9600, 18724, 0, 9600, 18724, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
