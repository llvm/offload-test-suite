    Data: [2304, 16, 0, 4928, 1040, 0, 4928, 1040, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 2304, 16, 0, 4928, 1040, 0, 4928, 1040, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0, 6080, 21845, 0]
  - Name: _wave_op_index
