    Stride: 4
    Fill: 0
    Size: 90
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 2432, 32769, 0, 2432, 32769, 0, 2448, 32769, 0, 2448, 32769, 0, 2464, 32769, 0, 2464, 32769, 0, 2880, 64, 0, 2896, 64, 0, 2912, 64, 0, 3456, 1040, 0, 3456, 1040, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 2432, 32769, 0, 2432, 32769, 0, 2448, 32769, 0, 2448, 32769, 0, 2464, 32769, 0, 2464, 32769, 0, 2880, 64, 0, 2896, 64, 0, 2912, 64, 0, 3456, 1040, 0, 3456, 1040, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
