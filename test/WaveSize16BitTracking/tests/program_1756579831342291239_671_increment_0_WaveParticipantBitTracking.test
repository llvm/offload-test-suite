    Data: [576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 1920, 32768, 0, 3088, 32768, 0, 3104, 32768, 0, 3792, 32768, 0, 3808, 32768, 0, 5312, 520, 0, 5312, 520, 0, 5328, 520, 0, 5328, 520, 0, 10576, 32768, 0, 10592, 32768, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 1920, 32768, 0, 3088, 32768, 0, 3104, 32768, 0, 3792, 32768, 0, 3808, 32768, 0, 5312, 520, 0, 5312, 520, 0, 5328, 520, 0, 5328, 520, 0, 10576, 32768, 0, 10592, 32768, 0]
