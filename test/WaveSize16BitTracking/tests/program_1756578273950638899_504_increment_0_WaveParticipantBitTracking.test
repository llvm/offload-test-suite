Shaders:
  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]  # Single dispatch for 32 threads
Buffers:
  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 78
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [3200, 128, 0, 3216, 128, 0, 3232, 128, 0, 4800, 47, 0, 4800, 47, 0, 4800, 47, 0, 4800, 47, 0, 4800, 47, 0, 5712, 1, 0, 5728, 1, 0, 8784, 16384, 0, 8800, 16384, 0, 9664, 32768, 0, 3200, 128, 0, 3216, 128, 0, 3232, 128, 0, 4800, 47, 0, 4800, 47, 0, 4800, 47, 0, 4800, 47, 0, 4800, 47, 0, 5712, 1, 0, 5728, 1, 0, 8784, 16384, 0, 8800, 16384, 0, 9664, 32768, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
  - Resources:
    - Name: _participant_bit
      Kind: RWStructuredBuffer
      DirectXBinding:
        Register: 0
        Space: 0
      VulkanBinding:
        Binding: 0
    - Name: _wave_op_index
