    Stride: 4
    Fill: 0
    Size: 84
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1344, 8192, 0, 4928, 1, 0, 7296, 64, 0, 8464, 55298, 0, 8464, 55298, 0, 8464, 55298, 0, 8464, 55298, 0, 8464, 55298, 0, 8480, 55298, 0, 8480, 55298, 0, 8480, 55298, 0, 8480, 55298, 0, 8480, 55298, 0, 9152, 32768, 0, 1344, 8192, 0, 4928, 1, 0, 7296, 64, 0, 8464, 55298, 0, 8464, 55298, 0, 8464, 55298, 0, 8464, 55298, 0, 8464, 55298, 0, 8480, 55298, 0, 8480, 55298, 0, 8480, 55298, 0, 8480, 55298, 0, 8480, 55298, 0, 9152, 32768, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
