    Stride: 4
    Fill: 0
    Size: 84
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [4480, 2, 0, 4496, 2, 0, 4512, 2, 0, 4928, 2, 0, 4944, 2, 0, 4960, 2, 0, 5248, 17476, 0, 5248, 17476, 0, 5248, 17476, 0, 5248, 17476, 0, 5696, 34952, 0, 5696, 34952, 0, 5696, 34952, 0, 5696, 34952, 0, 4480, 2, 0, 4496, 2, 0, 4512, 2, 0, 4928, 2, 0, 4944, 2, 0, 4960, 2, 0, 5248, 17476, 0, 5248, 17476, 0, 5248, 17476, 0, 5248, 17476, 0, 5696, 34952, 0, 5696, 34952, 0, 5696, 34952, 0, 5696, 34952, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
