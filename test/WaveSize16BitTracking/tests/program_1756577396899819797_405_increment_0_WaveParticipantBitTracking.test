    Stride: 4
    Fill: 0
    Size: 78
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 1216, 8, 0, 2112, 2080, 0, 2112, 2080, 0, 3648, 4353, 0, 3648, 4353, 0, 3648, 4353, 0, 3392, 32768, 0, 5968, 64, 0, 5984, 64, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 576, 85, 0, 1216, 8, 0, 2112, 2080, 0, 2112, 2080, 0, 3648, 4353, 0, 3648, 4353, 0, 3648, 4353, 0, 3392, 32768, 0, 5968, 64, 0, 5984, 64, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
Results:
  - Result: BitTrackingValidation
    Rule: BufferParticipantPattern
    GroupSize: 3
    Actual: _participant_bit
    Expected: expected_bit_patterns
DescriptorSets:
