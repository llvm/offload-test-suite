  - Stage: Compute
    Entry: main
    DispatchSize: [1, 1, 1]  # Single dispatch for 32 threads
Buffers:
  - Name: _participant_bit
    Format: UInt32
    Stride: 4
    Fill: 0
    Size: 42
  - Name: expected_bit_patterns
    Format: UInt32
    Stride: 4
    Data: [1280, 1, 0, 8064, 17476, 0, 8064, 17476, 0, 8064, 17476, 0, 8064, 17476, 0, 9216, 32776, 0, 9216, 32776, 0, 1280, 1, 0, 8064, 17476, 0, 8064, 17476, 0, 8064, 17476, 0, 8064, 17476, 0, 9216, 32776, 0, 9216, 32776, 0]
  - Name: _wave_op_index
    Format: UInt32
    Stride: 4
    Data: [0]
